{#{% extends 'base.html.twig' %}#}

<!DOCTYPE html>
<html>
<head>

    <meta charset="UTF-8" />

    <title>{% block title %}CorinneCréation{% endblock %}</title>


    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <!--Import Google Icon Font-->
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Crete+Round|Days+One|Fredoka+One|Nunito|Parisienne|Pinyon+Script|Rochester|Sacramento|Sofia" rel="stylesheet">

    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/corinne/css/materialize.min.css') }}" media="screen,projection">
    {#<link type="text/css" rel="stylesheet" href="{{ asset('bundles/corinne/css/main.css') }}">#}

    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/corinne/css/materialize.min.css') }}" media="screen,projection">
    {#<link type="text/css" rel="stylesheet" href="{{ asset('bundles/corinne/css/main.css') }}">#}

</head>

{% block body %}
<div class="container">
    <div class="card teal center">
    <h1 class="white-text">Créer un Contact :</h1>
    </div>
    {{ form_start(form) }}
        {{ form_widget(form) }}
        <input class="waves-effect waves-light btn" style="width:220px" type="submit" value="Valider" />
    {{ form_end(form) }}

    <ul>
        <li>
            <a href="{{ path('contact_index') }}" class="waves-effect waves-light btn" style="width:220px">Retour à la liste</a>
        </li>
    </ul>
</div>
{% endblock %}
